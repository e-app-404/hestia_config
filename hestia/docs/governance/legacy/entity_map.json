    {
      "version": "1.0",
      "date": "2025-03-31",
      "notes": "Initial subsystem to sensor/script ownership mapping",
      "entity_map": {
        "hermes": {
          "sensors": [
            "sensor.presence_confidence",
            "binary_sensor.home_presence",
            "sensor.hermes_config"
          ],
          "scripts": [
            "script.fallback_presence_detection",
            "script.validate_presence_config",
            "script.reset_presence_config",
            "script.hermes_configure_presence_detection",
            "script.analyze_presence_patterns",
            "script.rate_limited_presence_mode_change"
          ]
        },
        "athena": {
          "sensors": [
            "sensor.template_monitor_status",
            "sensor.athena_config",
            "sensor.msf_diagnostic",
            "sensor.unavailable_entities_counter",
            "sensor.missing_registry_entities"
          ],
          "scripts": [
            "script.attempt_device_recovery"
          ]
        },
        "theia": {
          "sensors": [
            "sensor.theia_config",
            "sensor.room_lighting_suitability_score",
            "sensor.circadian_phase"
          ],
          "helpers": [
            "input_boolean.auto_lighting_override"
          ]
        },
        "hephaestus": {
          "sensors": [
            "sensor.room_registry",
            "sensor.room_registry_last_updated",
            "sensor.room_config_store",
            "sensor.tool_cooldown_status_summary"
          ],
          "scripts": [
            "script.get_room_device",
            "script.get_room_lights",
            "script.get_room_timeout"
          ],
          "helpers": [
            "input_datetime.clio_last_run",
            "input_datetime.phanes_last_run",
            "input_number.tool_execution_cooldown"
          ]
        },
        "core": {
          "sensors": [
            "sensor.hestia_system",
            "sensor.hestia_config",
            "sensor.abstraction_layer",
            "sensor.sensor_registry",
            "sensor.light_device_registry",
            "sensor.device_registry",
            "sensor.room_registry",
            "sensor.entity_registry_check",
            "sensor.sensor_fallback_integrity"
          ],
          "scripts": [
            "script.perform_system_health_check",
            "script.generate_system_health_report"
          ]
        }
      }
    },
